from random import randint
s=[]
l=[]
A=4
B=3
a=[]
for i in range(A):
    b=[]
    for i in range(B):
        b.append(randint(-100,100))
    a.append(b)
for symbol in a:
    print(*(i if i >=0 else '-' for i in symbol))
for i in range(len(a)):
    stroka=0
    for j in range(len(a[0])):
        stroka+=a[i][j]
    s.append(stroka)
for i in range(len(a[0])):
    suma=0
    for j in range(len(a)):
        suma=a[j][i]
    l.append(suma)
print(s)
print(l)


